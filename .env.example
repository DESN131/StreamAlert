TELEGRAM_BOT_TOKEN=123456789:xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TELEGRAM_CHAT_ID=123456789

# 录播姬配置的 Webhook 路径，默认 /webhook
WEBHOOK_PATH=/webhook

# Flask 服务配置
FLASK_HOST=0.0.0.0
FLASK_PORT=5000
FLASK_DEBUG=false

# 请求 Telegram API 超时时间（秒）
REQUEST_TIMEOUT_SECONDS=8

# EventId 去重缓存保留时间（秒）
EVENT_TTL_SECONDS=86400